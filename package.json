{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "jest --config test/jest.lint.ts",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx|jsx|css|scss)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format test build",
    "check-types": "tsc",
    "lint-staged": "lint-staged",
    "test": "is-ci-cli \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:coverage-open": "npm run test:coverage && open coverage/lcov-report/index.html",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "lint-staged": {
    "**/*.+(js|json|css|scss|ts|tsx)": [
      "prettier",
      "jest --findRelatedTests"
    ]
  },
  "dependencies": {
    "@mdx-js/react": "^2.3.0",
    "@types/react-modal": "^3.13.1",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint-config-next": "^13.2.4",
    "next": "^13.2.4",
    "npx": "^10.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-medium-image-zoom": "^5.0.1",
    "react-modal": "^3.16.1",
    "storybook": "^6.5.16",
    "storybook-addon-next-router": "^4.0.2",
    "styled-components": "^5.3.9",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.0-rc.1",
    "@storybook/manager-webpack5": "^6.5.0-rc.1",
    "@storybook/react": "^6.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/jest-axe": "^3.5.1",
    "@types/node": "^14.17.4",
    "@types/react": "^17.0.11",
    "@types/styled-components": "^5.1.19",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-loader": "^8.2.2",
    "canvas": "^2.8.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^7.0.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.0.5",
    "jest-axe": "^4.1.0",
    "jest-runner-eslint": "^0.10.1",
    "jest-styled-components": "^7.0.4",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^10.5.4",
    "msw": "^0.26.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.3.4",
    "webpack": "^5.74.0"
  }
}
